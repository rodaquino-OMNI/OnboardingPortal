{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "eslint . --config .eslintrc.practical.json --ext .ts,.tsx,.js,.jsx",
    "lint:strict:fix": "eslint . --config .eslintrc.practical.json --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "clean": "rm -rf .next out node_modules",
    "analyze": "ANALYZE=true next build",
    "test": "jest --watch --maxWorkers=50%",
    "test:ci": "jest --ci --coverage --maxWorkers=50% --passWithNoTests",
    "test:unit": "jest --testPathIgnorePatterns='__tests__/integration|__tests__/performance'",
    "test:integration": "jest --testPathPattern='__tests__/integration'",
    "test:performance": "jest --config=jest.perf.config.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:a11y": "npm run build && npm run test:a11y:only",
    "test:a11y:only": "pa11y-ci --config .pa11yci",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "bundle:analyze": "ANALYZE=true npm run build",
    "mobile:test": "playwright test --project=mobile",
    "pwa:build": "npm run build && workbox generateSW",
    "quality:check": "npm run type-check && npm run lint:strict && npm run format:check && npm run test:ci",
    "quality:validate": "node scripts/validate-build-quality.js",
    "quality:fix": "npm run lint:strict:fix && npm run format",
    "precommit": "lint-staged",
    "prepare": "husky"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@next/swc-darwin-arm64": "^15.3.5",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.7.0",
    "chart.js": "^4.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.11.0",
    "lucide-react": "^0.525.0",
    "next": "14.2.30",
    "next-pwa": "^5.6.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "^7.60.0",
    "tailwind-merge": "^2.5.4",
    "tesseract.js": "^5.1.1",
    "zod": "^3.24.1",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@faker-js/faker": "^9.3.0",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.7",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.30",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.1.2",
    "pa11y-ci": "^2.4.2",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
