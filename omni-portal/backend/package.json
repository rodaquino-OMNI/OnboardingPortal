{
  "name": "omni-portal-backend",
  "version": "1.0.0",
  "description": "Laravel 10 backend API for the Omni Onboarding Portal - A comprehensive healthcare employee onboarding platform",
  "main": "webpack.mix.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "playwright test --config=.playwrightrc.ts",
    "test:e2e:headed": "playwright test --config=.playwrightrc.ts --headed",
    "test:e2e:debug": "playwright test --config=.playwrightrc.ts --debug",
    "test:e2e:ui": "playwright test --config=.playwrightrc.ts --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:chrome": "playwright test --config=.playwrightrc.ts --project=chromium",
    "test:e2e:firefox": "playwright test --config=.playwrightrc.ts --project=firefox",
    "test:e2e:safari": "playwright test --config=.playwrightrc.ts --project=webkit",
    "test:e2e:mobile": "playwright test --config=.playwrightrc.ts --project='Mobile Chrome' --project='Mobile Safari'",
    "test:setup": "php artisan migrate:fresh --env=testing --seed && php artisan db:seed --class=DemoUserSeeder --env=testing"
  },
  "keywords": [
    "laravel",
    "healthcare",
    "onboarding",
    "e2e-testing",
    "playwright"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "ws": "^8.18.3"
  }
}
