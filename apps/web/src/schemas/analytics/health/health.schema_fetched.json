{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://omni-portal.example.com/schemas/analytics/health/schema_fetched.json",
  "title": "Health Questionnaire Schema Fetched Event",
  "description": "Analytics event for health questionnaire schema fetch operations (ADR-004 compliant, no PHI)",
  "type": "object",
  "required": [
    "event_name",
    "timestamp",
    "user_hash",
    "version",
    "fetch_latency_ms"
  ],
  "properties": {
    "event_name": {
      "const": "health.schema_fetched",
      "description": "Event identifier"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "description": "ISO 8601 timestamp of the event"
    },
    "user_hash": {
      "type": "string",
      "pattern": "^[a-f0-9]{64}$",
      "description": "SHA-256 hash of user_id for de-identified tracking"
    },
    "session_id": {
      "type": "string",
      "format": "uuid",
      "description": "Unique session identifier (not user-identifying)"
    },
    "version": {
      "type": "integer",
      "minimum": 1,
      "description": "Questionnaire schema version number"
    },
    "fetch_latency_ms": {
      "type": "number",
      "minimum": 0,
      "description": "API response time in milliseconds"
    },
    "feature_flag": {
      "type": "boolean",
      "description": "Whether feature flag for new schema is enabled"
    },
    "cache_hit": {
      "type": "boolean",
      "description": "Whether schema was served from cache"
    },
    "source": {
      "type": "string",
      "enum": ["api", "cache", "fallback"],
      "description": "Source of the schema data"
    }
  },
  "additionalProperties": false
}
